{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "es6",
        "moduleResolution": "node",
        "target": "es5",
        "allowJs": true,
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
          "public/*": ["public/*"],
          "src/*": ["src/*"],
        }
    },
    "include": [
        "./src/**/*",
        "src/custom.d.ts"
    ],
}