{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "react-hot-loader/babel",
        [
          "@emotion",
          {
            "sourceMap": false,
            "autoLabel": "always",
            "labelFormat": "[filename]__[local]"
          }
        ]
    ]
}